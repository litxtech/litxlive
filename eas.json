{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "store",
      "channel": "preview",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "app-bundle",
        "env": {
          "NODE_ENV": "production"
        },
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "production-small": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "app-bundle",
        "env": {
          "NODE_ENV": "production",
          "EXPO_OPTIMIZE": "true"
        },
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "internal"
      }
    },
    "beta": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "beta"
      }
    }
  }
}
