{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --tunnel",
    "start-web": "expo start --web --tunnel",
    "start-web-dev": "DEBUG=expo* expo start --web --tunnel",
    "lint": "expo lint",
    "test": "echo 'No tests specified'",
    "build:web": "expo export --platform web --output-dir dist",
    "vercel-build": "npm run build:web",
    "dev": "expo start -c --tunnel",
    "auto-push": "powershell -ExecutionPolicy Bypass -File ./auto-push.ps1",
    "push": "git add . && git commit -m 'feat: Otomatik commit' && git push origin main",
    "eas-build": "eas build --platform all --non-interactive",
    "build:android": "eas build --platform android --profile production",
    "build:android:auto": "eas build --platform android --profile production --auto-submit",
    "build:android:dev": "eas build --platform android --profile development --non-interactive",
    "build:android:preview": "eas build --platform android --profile preview --non-interactive",
    "build:android:production": "eas build --platform android --profile production --non-interactive",
    "submit:android": "eas submit --platform android --latest",
    "submit:android:internal": "eas submit --platform android --latest --track internal",
    "submit:android:beta": "eas submit --platform android --latest --track beta",
    "submit:android:production": "eas submit --platform android --latest --track production",
    "deploy:google-play": "npm run build:android:auto",
    "ci:build:dev": "npm ci && npm run build:android:dev",
    "ci:build:preview": "npm ci && npm run build:android:preview",
    "ci:build:production": "npm ci && npm run build:android:production"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.71",
    "@expo/vector-icons": "^15.0.2",
    "@hono/node-server": "^1.19.5",
    "@hono/trpc-server": "^0.4.0",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.6",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@stripe/stripe-react-native": "0.50.3",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@tanstack/react-query": "^5.90.3",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@ungap/structured-clone": "^1.3.0",
    "@vercel/node": "^5.3.24",
